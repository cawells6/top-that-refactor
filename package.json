{
  "name": "top-that-refactor",
  "version": "1.0.0",
  "description": "Refactored Top That! card game server and utilities",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "nodemon --watch ./ --ext ts --exec \"node --loader ts-node/esm ./server.ts\"",
    "force": "node --loader ts-node/esm ./scripts/force-dev-start.ts",
    "dev:force": "node --loader ts-node/esm ./scripts/force-dev-start.ts",
    "test": "jest",
    "dev:client": "vite",
    "build:client": "vite build",
    "preview:client": "vite preview"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.14",
    "@types/socket.io-client": "^1.4.36",
    "babel-jest": "^29.7.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
