{
  "name": "top-that-refactor",
  "version": "1.0.0",
  "description": "Refactored Top That! card game server and utilities",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "npx ts-node-dev --respawn --transpile-only --watch server.ts --watch controllers/**/*.ts --watch models/**/*.ts --watch src/**/*.ts server.ts",
    "force": "npx ts-node ./scripts/force-dev-start.ts",
    "dev:force": "npx ts-node ./scripts/force-dev-start.ts",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}